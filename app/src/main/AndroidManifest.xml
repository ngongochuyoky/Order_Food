<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.orderfood">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".SuccessActivity"></activity>
        <activity android:name=".ReviewAndPayActivity" />
        <activity android:name=".InforBuyerActivity" />
        <activity
            android:name=".UpdateInfoActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".ProductActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".CartActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".TypeProductActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".DetailProductActivity"
            android:parentActivityName=".ProductActivity" />
        <activity android:name=".UserInfoActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".AuthActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>